# Application settings
ENVIRONMENT=production
VERSION=1.0
RUST_LOG=better_call_put=debug,hyper=info

# Resource limits
CPU_LIMIT=4
MEMORY_LIMIT=4G
CPU_RESERVATION=2
MEMORY_RESERVATION=2G

# Healthcheck settings
HEALTHCHECK_INTERVAL=30s
HEALTHCHECK_TIMEOUT=10s
HEALTHCHECK_RETRIES=3

# Logging settings
LOG_MAX_SIZE=100m
LOG_MAX_FILES=10